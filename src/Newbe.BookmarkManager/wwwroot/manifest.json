{
  "manifest_version": 2,
  "name": "Amazing Favorites",
  "version": "0.6.1",
  "description": "Amazing Favorites help you to manage you bookmarks efficiently",
  "background": {
    "page": "index.html?path=background",
    "persistent": true
  },
  "icons": {
    "16": "icons/16icon.png",
    "48": "icons/48icon.png",
    "128": "icons/128icon.png"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval' 'wasm-eval' 'sha256-v8v3RKRPmN4odZ1CWM5gw80QKPCCWMcpNeOmimNL2AA=' 'sha256-j0z5Z6xOoSHTeg6zVPMoHNt5D5v+7IWgT9m5uC9mrGg=' 'sha256-SQC3opwSNRzP3qBB9CWB9I3347XafmEdFyNx7sU8EhI=' 'sha256-ekPKVeJX0sFL97OO7iucY3R3j3qdTIvUSwffdbcbxjA=' 'sha256-Ku0DBFoNk825E3GZZ7OLqYiLPXgpfFVkmlpLYzy52Q0=' 'sha256-RXfkN4imQn+87yqYyEUBoeMJmcNRWSMfgv80LKZxKbc=' https://js.monitor.azure.com/scripts/b/ai.2.min.js ; object-src 'self'",
  "web_accessible_resources": [
    "framework/*",
    "BrowserExtensionScripts/*",
    "WebExtensionsScripts/*"
  ],
  "permissions": [
    "tabs",
    "bookmarks",
    "commands"
  ],
  "commands": {
    "open-manager-ui": {
      "suggested_key": {
        "default": "Alt+S",
        "windows": "Alt+S",
        "mac": "Alt+S"
      },
      "description": "open-manager-ui"
    }
  },
  "options_page": "/manager/index.html",
  "browser_action": {
    "default_popup": "popup.html"
  }
}